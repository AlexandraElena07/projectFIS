<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="392.0" prefWidth="337.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfis.EditEventsController">
    <children>
        <AnchorPane layoutY="-2.0" prefHeight="392.0" prefWidth="338.0">
            <children>
                <Label alignment="CENTER" layoutX="27.0" layoutY="39.0" prefHeight="47.0" prefWidth="290.0" text="Editare Detalii Eveniment" textFill="#f76300">
                    <font>
                        <Font name="Gigi" size="22.0" />
                    </font>
                </Label>
                <Label layoutX="15.0" layoutY="194.0" prefHeight="24.0" prefWidth="82.0" text="Descriere" textFill="#f76300">
                    <font>
                        <Font name="Algerian" size="15.0" />
                    </font>
                </Label>
                <TextField fx:id="descriere" layoutX="113.0" layoutY="193.0" prefHeight="26.0" prefWidth="210.0" promptText="Descriere">
                    <font>
                        <Font name="Bodoni MT Black" size="12.0" />
                    </font>
                </TextField>
                <Button fx:id="salvare" layoutX="18.0" layoutY="323.0" mnemonicParsing="false" onAction="#save" prefHeight="26.0" prefWidth="144.0" style="-fx-background-color: #f76300;" text="Salvare" textFill="WHITE">
                    <font>
                        <Font name="Century Gothic" size="24.0" />
                    </font>
                </Button>
                <Button fx:id="inchidere" layoutX="172.0" layoutY="323.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="26.0" prefWidth="144.0" style="-fx-background-color: #000000;" text="Inchidere" textFill="WHITE">
                    <font>
                        <Font name="Century Gothic" size="24.0" />
                    </font>
                </Button>
                <Label fx:id="editLabel" alignment="CENTER" layoutX="36.0" layoutY="258.0" prefHeight="26.0" prefWidth="302.0" textFill="#f76300" />
                <Label alignment="CENTER" layoutX="15.0" layoutY="139.0" prefHeight="24.0" prefWidth="82.0" text="ID" textFill="#f76300">
                    <font>
                        <Font name="Algerian" size="24.0" />
                    </font>
                </Label>
                <TextField fx:id="id" alignment="BOTTOM_LEFT" layoutX="113.0" layoutY="140.0" prefHeight="26.0" prefWidth="210.0" promptText="ID">
                    <font>
                        <Font name="Broadway" size="12.0" />
                    </font>
                </TextField>
            </children>
        </AnchorPane>
    </children>
</Pane>
