<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="272.0" prefWidth="490.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfis.AfisareVoturiController">
   <children>
      <AnchorPane layoutY="1.0" prefHeight="254.0" prefWidth="197.0">
         <children>
            <ImageView fx:id="leu1Image" fitHeight="287.0" fitWidth="250.0" layoutX="-7.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../Evenimente/leu1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="back" layoutX="8.0" layoutY="240.0" mnemonicParsing="false" onAction="#cancelButtonOnAction" prefHeight="26.0" prefWidth="70.0" style="-fx-background-color: #000000;" text="Închidere" textFill="WHITE" />
            <Button fx:id="actualizare" layoutX="161.0" layoutY="240.0" mnemonicParsing="false" onAction="#refreshTable" prefHeight="26.0" prefWidth="76.0" style="-fx-background-color: #f76300;" text="Actualizare" textFill="WHITE" />
         </children>
      </AnchorPane>
      <TableView fx:id="table" layoutX="240.0" layoutY="-2.0" prefHeight="275.0" prefWidth="250.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="32.00007629394531" text="ID" />
          <TableColumn fx:id="name" prefWidth="107.199951171875" text="Nume propus" />
            <TableColumn fx:id="count" prefWidth="109.19998168945312" text="Număr voturi" />
        </columns>
      </TableView>
   </children>
</Pane>
